			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body class="container">
		<header>
			<nav>
				<ul>
					<li>
{#						<strong>Logo</strong>#}
					</li>
				</ul>
				<ul>
					<li>
						<a href="{{ path('app_bande_index') }}">Accueil</a>
					</li>
					<li>
						<a href="/bande">Depense hors bande</a>
					</li>
				</ul>
			</nav>
		</header>
		<main>
			{% block body %}{% endblock %}
		</main>
	</body>
</html>
